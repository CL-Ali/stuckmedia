{
    "version": 2,
    "builds": [
      {
        "src": "stuckmedia/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "50mb"
          }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "static/$1"
      },
  
      {
        "src": "/(.*)",
        "dest": "stuckmedia/wsgi.py"
      }
    ]
  }
   
         