{
    "version": 2,
    "builds": [
      {
        "src": "stuckmedia/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "50mb"
          }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "static/"
      },
      {
        "src": "/(.*)",
        "dest": "stuckmedia/wsgi.py"
      }
    ]
  }
   
         