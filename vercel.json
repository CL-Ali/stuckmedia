{
    "version": 2,
    "builds": [
      {
        "src": "stuckmedia/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9",
          "buildCommand": "python -m venv venv && source venv/bin/activate && pip install -r requirements.txt"

        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
     
      {
        "src": "/(.*)",
        "dest": "stuckmedia/wsgi.py"
      }
    ]
  }
  